{
  "name": "julibo/msfoole-alone",
  "description": "基于swoole4的高性能API框架",
  "type": "library",
  "keywords": [
    "msfoole-alone",
    "msfoole",
    "swoole",
    "framework"
  ],
  "homepage": "http://msfoole.julibo.com/",
  "license": "Apache-2.0",
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "authors": [
    {
      "name": "carson",
      "email": "yuzhanwei@aliyun.com",
      "homepage": "http://msfoole.julibo.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-swoole": "~4.3.0",
    "symfony/console": "~4.2.3",
    "symfony/yaml": "~4.2.3",
    "chency147/cli-message": "~0.1.1",
    "topthink/think-log": "~1.0",
    "topthink/think-container": "~1.0",
    "topthink/think-helper": "~1.0"
